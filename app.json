{
  "expo": {
    "name": "aotd",
    "slug": "adventure_of_the_day",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/adaptive-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "AoTD"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/adaptive-icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.lucidmach.aotd"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icons/favicon.ico"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/JustAnotherHand-Regular.ttf",
            "./assets/fonts/Comfortaa-Regular.ttf",
            "./assets/fonts/Comfortaa-Bold.ttf",
            "./assets/fonts/Comfortaa-Light.ttf",
            "./assets/fonts/Comfortaa-Medium.ttf",
            "./assets/fonts/Comfortaa-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b0c3bff9-3d95-4865-8499-1a0cefffc334"
      }
    }
  }
}
